#include<iostream>
#include<cmath>
using namespace std;
#define M 1000

int getFactor(int n);

int main() {
	for (int i = 2; i < M; i++) {
		if (getFactor(i) == i) {
			cout << i << endl;
		}
	}
}

int getFactor(int n) {
	int max = (int) sqrt(n);
	int sum = 0;
	if (max * max == n) {
		sum += max;
		for (int i = 1; i < max; i++) {
			if (n % i == 0) {
				sum += i;
				sum += n / i;
			}
		}
	} else {
		for (int i = 1; i <= max; i++) {
			if (n % i == 0) {
				sum += i;
				sum += n / i;
			}
		}
	}
	sum -= n;
	return sum;
}
